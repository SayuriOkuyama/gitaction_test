{
  "name": "reirei-anpan/apprentice-cicd",
  "description": "A short description of your project",
  "license": "MIT",
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "phpcs": [
      "./vendor/bin/phpcs --standard=phpcs.xml src tests"
    ],
    "phpunit": [
      "./vendor/bin/phpunit tests"
    ]
  }
}
